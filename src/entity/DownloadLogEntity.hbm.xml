<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.DownloadLogEntity" table="download_log" schema="video">
        <id name="downloadId">
            <column name="download_id" sql-type="int(11)"/>
        </id>
        <property name="downloadDate">
            <column name="download_date" sql-type="datetime"/>
        </property>
        <many-to-one name="user" column="download_id" class="entity.UserEntity">

        </many-to-one>
        <many-to-one name="video" class="entity.VideoEntity" column="download_id">

        </many-to-one>
    </class>
</hibernate-mapping>